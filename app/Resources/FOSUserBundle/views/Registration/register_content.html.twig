{% trans_default_domain 'FOSUserBundle' %}


{{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
    <h5>Форма создания пользователя</h5>
    {{ form_widget(form) }}
    <div>
        <input type="reset" class="btn-flat grey-text waves-effect" value="Сбросить">
        <input class="btn indigo accent-3 waves-effect waves-light" type="submit" value="{{ 'registration.submit'|trans }}" />
    </div>
{{ form_end(form) }}
